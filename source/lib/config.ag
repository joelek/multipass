guard ProviderDynu: {
	"type": "dynu",
	"key": string
};

guard ProviderGlesys: {
	"type": "glesys",
	"account": string,
	"key": string
};

guard ProviderLoopia: {
	"type": "loopia",
	"username": string,
	"password": string,
	"account"?: string
};

guard Provider: ProviderDynu | ProviderGlesys | ProviderLoopia;

guard Certificate: {
	"hostnames": string[],
	"root"?: string,
	"account_key"?: string,
	"account_pass"?: string,
	"certificate_key"?: string,
	"certificate_pass"?: string,
	"certificate"?: string
};

guard Options: {
	"providers": Provider[],
	"certificates": Certificate[],
	"acme"?: string,
	"monitor"?: boolean
};
